<div id="index-form" class="form-part">
    <div class="form-containor">
      <div class="form-header">
        <h2>Create Project</h2>
        <div id="remove">
          X
        </div>
      </div>
      <div class="form-inner">
        <form action="api/create/project" method="post">
            <input type="hidden" name="issu" value="<%=Issu._id%>">
          <div class="input_list">
            <label for="title">Title</label>
            <input type="text" name="name" placeholder="title" required>
          </div>
          <div class="input_list">
            <label for="Author">Author</label>
            <input type="text" name="author" placeholder="author" required>
          </div>
          <div class="input_list">
            <label for="Description">Description</label>
            <textarea name="description" id="description"  placeholder="description" required></textarea>
          </div>
          <div class="input_list">
            <label for="labels">select option</label>
            <select name="labels" id="dropdown" required>
              <option value="">Select Labels</option>
                <option value="UI">UI</option>
                <option value="Bug">Bug</option>
                <option value="Development">Development</option>
                <option value="Backend">Backend</option>
                <option value="FrontEnd">FrontEnd</option>
                <option value="Database">Database</option>
              </select>
          </div>
          <div class="input_list">
            <input type="submit" value="submit">
          </div>
        </form>
      </div>
    </div>
  </div>